{
	"name": "JIRA Notifier for Chrome",
	"version": "${project.version}",
	"description": "Checks JIRA for issues assigned to you.",
	"permissions": [ "tabs", "http://*/*", "https://*/*", "history" ],
	"icons": {
      "128": "img/icon-128.png",
      "16": "img/icon-16.png",
      "32": "img/icon-32.png",
      "64": "img/icon-64.png"
   },
   "browser_action": {
      "default_icon": "img/icon-signed-out.png",
      "popup": "popup.html"
   },
   "background_page": "background.html",
   "options_page": "options.html"
}